{
  "manifest_version": 2,
 
  "name": "Notetaker",
  "version": "0.1.0",
  "description": "Minimum Viable Produce for the notetaker chrome extension",
  
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "background" : {
    "scripts" : ["./index.js"]
  },
  
  "permissions": [
    "storage",
    "tabs",
    "storage"
  ]
}
